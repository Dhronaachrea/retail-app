<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_weight="3"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/tv_user_name"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@drawable/table_content_cell_bg"
        android:text="@string/player"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_total_wager"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/table_content_cell_bg"
        android:text="@string/wager"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_total_winning"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:background="@drawable/table_content_cell_bg"
        android:text="@string/winning_ngr"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_bil"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:background="@drawable/table_content_cell_bg"
        android:text="@string/bonus"
        android:textSize="12sp" />
    <TextView
        android:id="@+id/tv_tax"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:background="@drawable/table_content_cell_bg"
        android:text="@string/tax"
        android:textSize="12sp" />
    <TextView
        android:id="@+id/tv_ngr"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:background="@drawable/table_content_cell_bg"
        android:text="@string/ngr"
        android:textSize="12sp" />

</LinearLayout>
